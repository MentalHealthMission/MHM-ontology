    <connect:hasValue rdf:datatype="http://www.w3.org/2001/XMLSchema#float">0.8</connect:hasValue> <!-- 80% of the time spent at home -->
    <connect:belongsToLayer rdf:resource="http://connectdigitalstudy.com/ontology#DerivedFeatureLayer"/>
  </rdf:Description>

