  <!-- Define Participant X -->
  <rdf:Description rdf:about="http://connectdigitalstudy.com/ontology#ParticipantX">
    <rdf:type rdf:resource="http://connectdigitalstudy.com/ontology#Participant"/>
    <connect:hasID rdf:datatype="http://www.w3.org/2001/XMLSchema#string">X123</connect:hasID>
    <connect:hasLabel rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Participant X</connect:hasLabel>
  </rdf:Description>

  <!-- Physiological Measurements for Participant X -->
  <rdf:Description rdf:about="http://connectdigitalstudy.com/ontology#HeartRateMeasurementX">
    <rdf:type rdf:resource="http://connectdigitalstudy.com/ontology#HeartRateMeasurement"/>
    <connect:hasValue rdf:datatype="http://www.w3.org/2001/XMLSchema#float">85</connect:hasValue>
    <connect:hasUnit rdf:datatype="http://www.w3.org/2001/XMLSchema#string">bpm</connect:hasUnit>
    <connect:wasGeneratedBy rdf:resource="http://connectdigitalstudy.com/ontology#WearableDeviceX"/>
    <connect:belongsToLayer rdf:resource="http://connectdigitalstudy.com/ontology#MeasurementLayer"/>
  </rdf:Description>

  <rdf:Description rdf:about="http://connectdigitalstudy.com/ontology#HeartRateVariabilityMeasurementX">
    <rdf:type rdf:resource="http://connectdigitalstudy.com/ontology#HeartRateVariabilityMeasurement"/>
    <connect:hasValue rdf:datatype="http://www.w3.org/2001/XMLSchema#float">30</connect:hasValue>
    <connect:hasUnit rdf:datatype="http://www.w3.org/2001/XMLSchema#string">ms</connect:hasUnit>
    <connect:wasGeneratedBy rdf:resource="http://connectdigitalstudy.com/ontology#WearableDeviceX"/>
    <connect:belongsToLayer rdf:resource="http://connectdigitalstudy.com/ontology#MeasurementLayer"/>
  </rdf:Description>

  <!-- Behavioral Measurements for Participant X -->
  <rdf:Description rdf:about="http://connectdigitalstudy.com/ontology#ActivityMeasurementX">
    <rdf:type rdf:resource="http://connectdigitalstudy.com/ontology#ActivityMeasurement"/>
    <connect:hasValue rdf:datatype="http://www.w3.org/2001/XMLSchema#float">45</connect:hasValue>
    <connect:hasUnit rdf:datatype="http://www.w3.org/2001/XMLSchema#string">minutes</connect:hasUnit>
    <connect:wasGeneratedBy rdf:resource="http://connectdigitalstudy.com/ontology#WearableDeviceX"/>
    <connect:belongsToLayer rdf:resource="http://connectdigitalstudy.com/ontology#MeasurementLayer"/>
  </rdf:Description>

  <rdf:Description rdf:about="http://connectdigitalstudy.com/ontology#SleepStageMeasurementX">
    <rdf:type rdf:resource="http://connectdigitalstudy.com/ontology#SleepStageMeasurement"/>
    <connect:hasStage rdf:resource="http://connectdigitalstudy.com/ontology#Stage1"/>
    <connect:hasValue rdf:datatype="http://www.w3.org/2001/XMLSchema#float">6</connect:hasValue> <!-- Total sleep time in hours -->
    <connect:hasUnit rdf:datatype="http://www.w3.org/2001/XMLSchema#string">hours</connect:hasUnit>
    <connect:wasGeneratedBy rdf:resource="http://connectdigitalstudy.com/ontology#SmartphoneDeviceX"/>
    <connect:belongsToLayer rdf:resource="http://connectdigitalstudy.com/ontology#MeasurementLayer"/>
  </rdf:Description>

  <!-- Derived Feature: Mobility Pattern for Participant X -->
  <rdf:Description rdf:about="http://connectdigitalstudy.com/ontology#MobilityPatternFeatureX">
    <rdf:type rdf:resource="http://connectdigitalstudy.com/ontology#MobilityPatternFeature"/>
    <connect:hasMeasurement rdf:resource="http://connectdigitalstudy.com/ontology#ActivityMeasurementX"/>
    <connect:hasMeasurement rdf:resource="http://connectdigitalstudy.com/ontology#SleepStageMeasurementX"/>
    <connect:belongsToLayer rdf:resource="http://connectdigitalstudy.com/ontology#DerivedFeatureLayer"/>
  </rdf:Description>

  <rdf:Description rdf:about="http://connectdigitalstudy.com/ontology#HomeStayDurationFeatureX">
