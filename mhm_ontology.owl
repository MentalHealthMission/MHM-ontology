         xmlns:dcterms="http://purl.org/dc/terms/"
         xmlns:qudt="http://qudt.org/schema/qudt/"
         xmlns:unit="http://qudt.org/vocab/unit/"
         xmlns:iao="http://purl.obolibrary.org/obo/"
         xml:base="http://connectdigitalstudy.com/ontology">

  <owl:Ontology rdf:about="http://connectdigitalstudy.com/ontology">
    <rdfs:label>ODIM-MH</rdfs:label>
    <owl:versionInfo>0.1.0-dev</owl:versionInfo>
    <dcterms:title>Ontology for Digital Markers in Mental Health (ODIM-MH)</dcterms:title>
    <dcterms:description>Ontology for organizing measurements, features, context, provenance, and digital phenotypes for wearable/device-based health studies.</dcterms:description>
    <dcterms:license>https://creativecommons.org/licenses/by/4.0/</dcterms:license>
    <!-- Keep the ontology in OWL DL: avoid importing PROV-O and SKOS (OWL Full).
         We will reference their IRIs without imports. -->
    <!-- No imports to keep DL validation focused on local axioms -->
  </owl:Ontology>

  <!-- Minimal dcterms declarations used as annotations -->
  <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/title"/>
  <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/description"/>
  <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/license"/>
  <!-- Minimal IAO declarations for definition annotation -->
  <owl:AnnotationProperty rdf:about="http://purl.obolibrary.org/obo/IAO_0000115"/>

  <!-- Minimal QUDT declarations (DL-safe references) -->
  <owl:Class rdf:about="http://qudt.org/schema/qudt/QuantityValue"/>
  <owl:ObjectProperty rdf:about="http://qudt.org/schema/qudt/quantityValue">
    <rdfs:label>quantity value</rdfs:label>
  </owl:ObjectProperty>
  <owl:DatatypeProperty rdf:about="http://qudt.org/schema/qudt/numericValue"/>
  <owl:ObjectProperty rdf:about="http://qudt.org/schema/qudt/unit"/>

  <!-- Convenience numeric value property -->
  <owl:DatatypeProperty rdf:about="http://connectdigitalstudy.com/ontology#numericValue">
    <rdfs:subPropertyOf rdf:resource="http://qudt.org/schema/qudt/numericValue"/>
    <rdfs:domain rdf:resource="http://qudt.org/schema/qudt/QuantityValue"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
    <rdfs:label>numeric value</rdfs:label>
  </owl:DatatypeProperty>

  <!-- Minimal SOSA declarations (DL-safe references) -->
  <owl:Class rdf:about="http://www.w3.org/ns/sosa/Observation"/>
  <owl:ObjectProperty rdf:about="http://www.w3.org/ns/sosa/observedProperty"/>
  <owl:ObjectProperty rdf:about="http://www.w3.org/ns/sosa/hasFeatureOfInterest"/>
  <owl:DatatypeProperty rdf:about="http://www.w3.org/ns/sosa/resultTime"/>

  <!-- Minimal SKOS declarations (DL-safe references) -->
  <owl:Class rdf:about="http://www.w3.org/2004/02/skos/core#Concept"/>
  <owl:Class rdf:about="http://www.w3.org/2004/02/skos/core#ConceptScheme"/>
  <owl:ObjectProperty rdf:about="http://www.w3.org/2004/02/skos/core#inScheme"/>
  <owl:ObjectProperty rdf:about="http://www.w3.org/2004/02/skos/core#broader"/>
  <owl:ObjectProperty rdf:about="http://www.w3.org/2004/02/skos/core#narrower"/>
  <owl:ObjectProperty rdf:about="http://www.w3.org/2004/02/skos/core#related"/>
  <owl:ObjectProperty rdf:about="http://www.w3.org/2004/02/skos/core#hasTopConcept"/>
  <owl:ObjectProperty rdf:about="http://www.w3.org/2004/02/skos/core#topConceptOf"/>
  <owl:AnnotationProperty rdf:about="http://www.w3.org/2004/02/skos/core#prefLabel"/>
  <owl:AnnotationProperty rdf:about="http://www.w3.org/2004/02/skos/core#altLabel"/>
  <owl:AnnotationProperty rdf:about="http://www.w3.org/2004/02/skos/core#definition"/>

  <!-- DL-safe annotation to tag classes with SKOS concepts -->
  <owl:AnnotationProperty rdf:about="http://connectdigitalstudy.com/ontology#skosTag">
    <rdfs:label>SKOS tag</rdfs:label>
    <rdfs:comment>DL-safe annotation linking ontology terms to SKOS concepts.</rdfs:comment>
  </owl:AnnotationProperty>

  <!-- Minimal declarations for PROV-O terms to keep OWL DL without importing -->
  <owl:Class rdf:about="http://www.w3.org/ns/prov#Entity"/>
  <owl:Class rdf:about="http://www.w3.org/ns/prov#Activity"/>
  <owl:Class rdf:about="http://www.w3.org/ns/prov#Agent"/>
  <owl:ObjectProperty rdf:about="http://www.w3.org/ns/prov#used"/>
  <owl:ObjectProperty rdf:about="http://www.w3.org/ns/prov#wasDerivedFrom"/>
  <owl:ObjectProperty rdf:about="http://www.w3.org/ns/prov#wasGeneratedBy"/>
  <owl:ObjectProperty rdf:about="http://www.w3.org/ns/prov#wasAttributedTo"/>
  <owl:ObjectProperty rdf:about="http://www.w3.org/ns/prov#wasAssociatedWith"/>

  <!-- Layer superclass -->
  <owl:Class rdf:about="http://connectdigitalstudy.com/ontology#OntologyLayer">
    <rdfs:label>Ontology Layer</rdfs:label>
    <rdfs:comment>A meta-class grouping the modeling layers in this ontology.</rdfs:comment>
  </owl:Class>

  <!-- Specific layers -->

  <owl:Class rdf:about="http://connectdigitalstudy.com/ontology#MeasurementLayer">
    <rdfs:subClassOf rdf:resource="http://connectdigitalstudy.com/ontology#OntologyLayer"/>
    <rdfs:label>Measurement Layer</rdfs:label>
    <rdfs:comment>Layer for raw observations/measurements captured from devices and apps.</rdfs:comment>
  </owl:Class>

<owl:Class rdf:about="http://connectdigitalstudy.com/ontology#Measurement">
    <connect:belongsToLayer rdf:resource="http://connectdigitalstudy.com/ontology#MeasurementLayer"/>
    <rdfs:label>Measurement</rdfs:label>
    <rdfs:comment>A single observation or data point recorded by a device or system.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/prov#Entity"/>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/sosa/Observation"/>
    <iao:IAO_0000115>A single observation recorded by a device or system, optionally linked to a QUDT QuantityValue and SOSA properties.</iao:IAO_0000115>
  </owl:Class>

