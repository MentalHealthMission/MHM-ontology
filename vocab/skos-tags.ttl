@prefix odim:    <http://connectdigitalstudy.com/ontology#> .
@prefix connect: <http://connectdigitalstudy.com/ontology#> .
@prefix skos:    <http://www.w3.org/2004/02/skos/core#> .

# DL-safe tagging of ontology classes with SKOS concepts

# Physiological measurements
odim:HeartRateMeasurement            odim:skosTag odim:PhysiologicalProperty .
odim:RespiratoryRateMeasurement      odim:skosTag odim:PhysiologicalProperty .
odim:HeartRateVariabilityMeasurement odim:skosTag odim:PhysiologicalProperty .

# Behavioral measurements
odim:ActivityMeasurement    odim:skosTag odim:BehavioralProperty .
odim:SleepStageMeasurement  odim:skosTag odim:BehavioralProperty .

# Environmental measurements
odim:EnvironmentalMeasurement odim:skosTag odim:EnvironmentalProperty .

# Questionnaire model (illustrative tags to domains)
odim:QuestionnaireEntry odim:skosTag odim:StressDomain .
odim:Question          odim:skosTag odim:MoodDomain , odim:SleepDomain .
odim:ResponseItem      odim:skosTag odim:SleepDomain .
