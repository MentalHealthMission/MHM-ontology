@prefix connect: <http://connectdigitalstudy.com/ontology#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix qudt: <http://qudt.org/schema/qudt/> .
@prefix unit: <http://qudt.org/vocab/unit/> .

# Example individuals corresponding to the ABox previously embedded in mhm_ontology.owl

connect:activityAndSleepFeature1 a connect:Feature ;
  rdfs:label "Activity and sleep feature 1" ;
  connect:hasMeasurement connect:activityMeasurement1 , connect:sleepStageMeasurement1 .

connect:activityMeasurement1 a connect:ActivityMeasurement ;
  rdfs:label "Activity measurement X" ;
  connect:hasValue 1000.0 .

connect:sleepStageMeasurement1 a connect:SleepStageMeasurement ;
  rdfs:label "Sleep stage measurement 1" ;
  connect:hasStage connect:Stage1 .

connect:Stage1 a connect:SleepStage ;
  rdfs:label "Stage 1" ;
  connect:hasStageName "Light Sleep"^^xsd:string .

connect:exerciseAndSleepFeature1 a connect:ExerciseAndSleepFeature ;
  rdfs:label "Exercise and sleep feature 1" ;
  connect:hasMeasurement connect:exerciseMeasurement1 , connect:sleepStageMeasurement1 .

connect:exerciseMeasurement1 a connect:ExerciseMeasurement ;
  rdfs:label "Exercise measurement X" ;
  connect:hasValue 120.0 .

connect:Stage2 a connect:SleepStage ;
  rdfs:label "Stage 2" ;
  connect:hasStageName "Deep Sleep"^^xsd:string .

# Participant X and measurements

connect:ParticipantX a connect:Participant ;
  rdfs:label "Participant X" ;
  connect:hasID "X123" ;
  connect:hasLabel "Participant X" .

connect:HeartRateMeasurementX a connect:HeartRateMeasurement ;
  rdfs:label "Heart rate measurement X" ;
  connect:observedProperty connect:HeartRate ;
  connect:featureOfInterest connect:ParticipantX ;
  connect:resultTime "2025-03-24T08:15:00"^^xsd:dateTime ;
  connect:hasValue 85.0 ;
  connect:hasUnit "bpm" ;
  qudt:quantityValue [ a qudt:QuantityValue ; qudt:numericValue 85.0 ; qudt:unit unit:BPM ] ;
  connect:wasGeneratedBy connect:WearableDeviceX ;
  connect:belongsToLayer connect:MeasurementLayer .

connect:HeartRateVariabilityMeasurementX a connect:HeartRateVariabilityMeasurement ;
  rdfs:label "HRV measurement X" ;
  connect:observedProperty connect:HeartRateVariability ;
  connect:featureOfInterest connect:ParticipantX ;
  connect:resultTime "2025-03-24T08:15:10"^^xsd:dateTime ;
  connect:hasValue 30.0 ;
  connect:hasUnit "ms" ;
  qudt:quantityValue [ a qudt:QuantityValue ; qudt:numericValue 30.0 ; qudt:unit unit:MilliSecond ] ;
  connect:wasGeneratedBy connect:WearableDeviceX ;
  connect:belongsToLayer connect:MeasurementLayer .

connect:ActivityMeasurementX a connect:ActivityMeasurement ;
  rdfs:label "Activity measurement X" ;
  connect:observedProperty connect:ActivityDuration ;
  connect:featureOfInterest connect:ParticipantX ;
  connect:resultTime "2025-03-24T22:00:00"^^xsd:dateTime ;
  connect:hasValue 45.0 ;
  connect:hasUnit "minutes" ;
  qudt:quantityValue [ a qudt:QuantityValue ; qudt:numericValue 45.0 ; qudt:unit unit:Minute ] ;
  connect:wasGeneratedBy connect:WearableDeviceX ;
  connect:belongsToLayer connect:MeasurementLayer .

connect:SleepStageMeasurementX a connect:SleepStageMeasurement ;
  rdfs:label "Sleep stage measurement X" ;
  connect:hasStage connect:Stage1 ;
  connect:observedProperty connect:SleepDuration ;
  connect:featureOfInterest connect:ParticipantX ;
  connect:resultTime "2025-03-25T06:30:00"^^xsd:dateTime ;
  connect:hasValue 6.0 ;
  connect:hasUnit "hours" ;
  qudt:quantityValue [ a qudt:QuantityValue ; qudt:numericValue 6.0 ; qudt:unit unit:Hour ] ;
  connect:wasGeneratedBy connect:SmartphoneDeviceX ;
  connect:belongsToLayer connect:MeasurementLayer .

# Observed property resources
connect:HeartRate a rdfs:Resource ; rdfs:label "Heart rate" .
connect:HeartRateVariability a rdfs:Resource ; rdfs:label "Heart rate variability" .
connect:ActivityDuration a rdfs:Resource ; rdfs:label "Activity duration" .
connect:SleepDuration a rdfs:Resource ; rdfs:label "Sleep duration" .

connect:MobilityPatternFeatureX a connect:MobilityPatternFeature ;
  rdfs:label "Mobility pattern feature X" ;
  connect:hasMeasurement connect:ActivityMeasurementX , connect:SleepStageMeasurementX ;
  connect:belongsToLayer connect:DerivedFeatureLayer .

connect:HomeStayDurationFeatureX a connect:MobilityPatternFeature ;
  rdfs:label "Home stay duration feature X" ;
  connect:hasValue 0.8 ;
  connect:belongsToLayer connect:DerivedFeatureLayer .

connect:ActivityPhenotypeX a connect:ActivityPhenotype ;
  rdfs:label "Activity phenotype X" ;
  connect:hasMeasurement connect:ActivityMeasurementX ;
  connect:belongsToLayer connect:DigitalPhenotypeLayer .

connect:PhysicalInactivityX a connect:PhysicalInactivity ;
  rdfs:label "Physical inactivity X" ;
  connect:hasMeasurement connect:ActivityMeasurementX ;
  connect:belongsToLayer connect:DigitalPhenotypeLayer .

# Context examples

connect:TimeIntervalX a connect:TimeInterval ;
  rdfs:label "Time interval X" ;
  connect:hasStartTime "2025-03-24T08:00:00"^^xsd:dateTime ;
  connect:hasEndTime   "2025-03-24T23:59:59"^^xsd:dateTime ;
  connect:belongsToLayer connect:ContextualLayer .

connect:EventWindowX a connect:EventWindow ;
  rdfs:label "Event window X" ;
  connect:occursDuring connect:TimeIntervalX ;
  connect:belongsToLayer connect:ContextualLayer .

connect:ContextualSituationX a connect:ContextualSituation ;
  rdfs:label "Contextual situation X" ;
  connect:hasLabel "Social Withdrawal" ;
  connect:belongsToLayer connect:ContextualLayer .

# Provenance examples (Entity → Activity → Agent)

# Declare example agents and interfaces
connect:WearableDeviceX a connect:WearableDevice ;
  rdfs:label "Wearable device X" .

connect:SmartphoneDeviceX a connect:SmartphoneDevice ;
  rdfs:label "Smartphone device X" .

connect:DataInterfaceX a connect:DataInterface ;
  rdfs:label "Data interface X" .

connect:AppInterfaceX a connect:AppInterface ;
  rdfs:label "App interface X" .

# Sensing activity used by measurements
connect:SensingActivityX a connect:SensingActivity ;
  rdfs:label "Sensing activity X" ;
  connect:usedDevice connect:WearableDeviceX ;
  connect:associatedWithDevice connect:WearableDeviceX .

# Link measurement to sensing activity (entity → activity)
connect:HeartRateMeasurementX connect:wasGeneratedBy connect:SensingActivityX .

# Computation producing a derived feature
connect:ComputationX a connect:Computation ;
  rdfs:label "Computation X" ;
  connect:associatedWithDevice connect:SmartphoneDeviceX ;
  connect:usedAppInterface connect:AppInterfaceX .

connect:DerivedFeatureX a connect:DerivedFeature ;
  rdfs:label "Derived feature X" ;
  connect:wasComputedBy connect:ComputationX ;
  connect:wasDerivedFrom connect:HeartRateMeasurementX .

# Dataset attributed to participant and using data interface
connect:DataSetX a connect:SensorDataSet ;
  rdfs:label "Sensor dataset X" ;
  connect:wasCollectedFrom connect:ParticipantX ;
  connect:wasAttributedTo connect:ParticipantX ;
  connect:usedDataInterface connect:DataInterfaceX .
