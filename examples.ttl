@prefix odim:    <http://connectdigitalstudy.com/ontology#> .
@prefix connect: <http://connectdigitalstudy.com/ontology#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix qudt: <http://qudt.org/schema/qudt/> .
@prefix unit: <http://qudt.org/vocab/unit/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .

# Example individuals corresponding to the ABox previously embedded in mhm_ontology.owl

odim:activityAndSleepFeature1 a odim:Feature ;
  rdfs:label "Activity and sleep feature 1" ;
  odim:hasMeasurement odim:activityMeasurement1 , odim:sleepStageMeasurement1 .

odim:activityMeasurement1 a odim:ActivityMeasurement ;
  rdfs:label "Activity measurement X" ;
  odim:hasValue 1000.0 .

odim:sleepStageMeasurement1 a odim:SleepStageMeasurement ;
  rdfs:label "Sleep stage measurement 1" ;
  odim:hasStage odim:Stage1 .

odim:Stage1 a odim:SleepStage ;
  rdfs:label "Stage 1" ;
  odim:hasStageName "Light Sleep"^^xsd:string .

odim:exerciseAndSleepFeature1 a odim:ExerciseAndSleepFeature ;
  rdfs:label "Exercise and sleep feature 1" ;
  odim:hasMeasurement odim:exerciseMeasurement1 , odim:sleepStageMeasurement1 .

odim:exerciseMeasurement1 a odim:ExerciseMeasurement ;
  rdfs:label "Exercise measurement X" ;
  odim:hasValue 120.0 .

odim:Stage2 a odim:SleepStage ;
  rdfs:label "Stage 2" ;
  odim:hasStageName "Deep Sleep"^^xsd:string .

# Participant X and measurements

odim:ParticipantX a odim:Participant ;
  rdfs:label "Participant X" ;
  odim:hasID "X123" ;
  odim:hasLabel "Participant X" .

odim:HeartRateMeasurementX a odim:HeartRateMeasurement ;
  rdfs:label "Heart rate measurement X" ;
  odim:observedProperty odim:HeartRate ;
  odim:featureOfInterest odim:ParticipantX ;
  odim:resultTime "2025-03-24T08:15:00"^^xsd:dateTime ;
  odim:hasValue 85.0 ;
  odim:hasUnit "bpm" ;
  qudt:quantityValue [ a qudt:QuantityValue ; qudt:numericValue 85.0 ; odim:numericValue 85.0 ; qudt:unit unit:BPM ] ;
  odim:wasGeneratedBy odim:WearableDeviceX ;
  odim:belongsToLayer odim:MeasurementLayer .

odim:HeartRateVariabilityMeasurementX a odim:HeartRateVariabilityMeasurement ;
  rdfs:label "HRV measurement X" ;
  odim:observedProperty odim:HeartRateVariability ;
  odim:featureOfInterest odim:ParticipantX ;
  odim:resultTime "2025-03-24T08:15:10"^^xsd:dateTime ;
  odim:hasValue 30.0 ;
  odim:hasUnit "ms" ;
  qudt:quantityValue [ a qudt:QuantityValue ; qudt:numericValue 30.0 ; odim:numericValue 30.0 ; qudt:unit unit:MilliSecond ] ;
  odim:wasGeneratedBy odim:WearableDeviceX ;
  odim:belongsToLayer odim:MeasurementLayer .

odim:ActivityMeasurementX a odim:ActivityMeasurement ;
  rdfs:label "Activity measurement X" ;
  odim:observedProperty odim:ActivityDuration ;
  odim:featureOfInterest odim:ParticipantX ;
  odim:resultTime "2025-03-24T22:00:00"^^xsd:dateTime ;
  odim:hasValue 45.0 ;
  odim:hasUnit "minutes" ;
  qudt:quantityValue [ a qudt:QuantityValue ; qudt:numericValue 45.0 ; odim:numericValue 45.0 ; qudt:unit unit:Minute ] ;
  odim:wasGeneratedBy odim:WearableDeviceX ;
  odim:belongsToLayer odim:MeasurementLayer .

odim:SleepStageMeasurementX a odim:SleepStageMeasurement ;
  rdfs:label "Sleep stage measurement X" ;
  odim:hasStage odim:Stage1 ;
  odim:observedProperty odim:SleepDuration ;
  odim:featureOfInterest odim:ParticipantX ;
  odim:resultTime "2025-03-25T06:30:00"^^xsd:dateTime ;
  odim:hasValue 6.0 ;
  odim:hasUnit "hours" ;
  qudt:quantityValue [ a qudt:QuantityValue ; qudt:numericValue 6.0 ; odim:numericValue 6.0 ; qudt:unit unit:Hour ] ;
  odim:wasGeneratedBy odim:SmartphoneDeviceX ;
  odim:belongsToLayer odim:MeasurementLayer .

# Observed property resources
odim:HeartRate a rdfs:Resource ; rdfs:label "Heart rate" .
odim:HeartRateVariability a rdfs:Resource ; rdfs:label "Heart rate variability" .
odim:ActivityDuration a rdfs:Resource ; rdfs:label "Activity duration" .
odim:SleepDuration a rdfs:Resource ; rdfs:label "Sleep duration" .

odim:MobilityPatternFeatureX a odim:MobilityPatternFeature ;
  rdfs:label "Mobility pattern feature X" ;
  odim:hasMeasurement odim:ActivityMeasurementX , odim:SleepStageMeasurementX ;
  odim:belongsToLayer odim:DerivedFeatureLayer .

odim:HomeStayDurationFeatureX a odim:MobilityPatternFeature ;
  rdfs:label "Home stay duration feature X" ;
  odim:hasValue 0.8 ;
  odim:belongsToLayer odim:DerivedFeatureLayer .

odim:ActivityPhenotypeX a odim:ActivityPhenotype ;
  rdfs:label "Activity phenotype X" ;
  odim:hasMeasurement odim:ActivityMeasurementX ;
  odim:belongsToLayer odim:DigitalPhenotypeLayer .

odim:PhysicalInactivityX a odim:PhysicalInactivity ;
  rdfs:label "Physical inactivity X" ;
  odim:hasMeasurement odim:ActivityMeasurementX ;
  odim:belongsToLayer odim:DigitalPhenotypeLayer .

# Context examples

odim:TimeIntervalX a odim:TimeInterval ;
  rdfs:label "Time interval X" ;
  odim:hasStartTime "2025-03-24T08:00:00"^^xsd:dateTime ;
  odim:hasEndTime   "2025-03-24T23:59:59"^^xsd:dateTime ;
  odim:belongsToLayer odim:ContextualLayer .

odim:EventWindowX a odim:EventWindow ;
  rdfs:label "Event window X" ;
  odim:occursDuring odim:TimeIntervalX ;
  odim:belongsToLayer odim:ContextualLayer .

odim:ContextualSituationX a odim:ContextualSituation ;
  rdfs:label "Contextual situation X" ;
  odim:hasLabel "Social Withdrawal" ;
  odim:belongsToLayer odim:ContextualLayer .

# Provenance examples (Entity → Activity → Agent)

# Declare example agents and interfaces
odim:WearableDeviceX a odim:WearableDevice ;
  rdfs:label "Wearable device X" .

odim:SmartphoneDeviceX a odim:SmartphoneDevice ;
  rdfs:label "Smartphone device X" .

odim:DataInterfaceX a odim:DataInterface ;
  rdfs:label "Data interface X" .

odim:AppInterfaceX a odim:AppInterface ;
  rdfs:label "App interface X" .

# Sensing activity used by measurements
odim:SensingActivityX a odim:SensingActivity ;
  rdfs:label "Sensing activity X" ;
  odim:usedDevice odim:WearableDeviceX ;
  odim:associatedWithDevice odim:WearableDeviceX .

# Link measurement to sensing activity (entity → activity)
odim:HeartRateMeasurementX odim:wasGeneratedBy odim:SensingActivityX .

# Computation producing a derived feature
odim:ComputationX a odim:Computation ;
  rdfs:label "Computation X" ;
  odim:associatedWithDevice odim:SmartphoneDeviceX ;
  odim:usedAppInterface odim:AppInterfaceX .

odim:DerivedFeatureX a odim:DerivedFeature ;
  rdfs:label "Derived feature X" ;
  odim:wasComputedBy odim:ComputationX ;
  odim:wasDerivedFrom odim:HeartRateMeasurementX .

# Dataset attributed to participant and using data interface
odim:DataSetX a odim:SensorDataSet ;
  rdfs:label "Sensor dataset X" ;
  odim:wasCollectedFrom odim:ParticipantX ;
  odim:wasAttributedTo odim:ParticipantX ;
  odim:usedDataInterface odim:DataInterfaceX .

