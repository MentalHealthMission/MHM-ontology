PREFIX odim:    <http://connectdigitalstudy.com/ontology#>
PREFIX qudt:    <http://qudt.org/schema/qudt/>
PREFIX unit:    <http://qudt.org/vocab/unit/>

# Success if each example measurement has a quantityValue with a unit
ASK WHERE {
  VALUES (?m ?u) {
    (odim:HeartRateMeasurementX            unit:BPM)
    (odim:HeartRateVariabilityMeasurementX unit:MilliSecond)
    (odim:ActivityMeasurementX             unit:Minute)
    (odim:SleepStageMeasurementX           unit:Hour)
  }
  ?m qudt:quantityValue [ qudt:numericValue ?v ; qudt:unit ?u ] .
}
