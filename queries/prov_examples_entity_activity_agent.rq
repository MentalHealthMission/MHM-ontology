PREFIX odim:    <http://connectdigitalstudy.com/ontology#>

ASK {
  # Measurement generated by sensing activity
  odim:HeartRateMeasurementX odim:wasGeneratedBy odim:SensingActivityX .

  # Computation uses app interface and produces derived feature
  odim:DerivedFeatureX odim:wasComputedBy odim:ComputationX .
  odim:ComputationX odim:usedAppInterface odim:AppInterfaceX .

  # Dataset attribution to participant and interface use
  odim:DataSetX odim:wasAttributedTo odim:ParticipantX .
  odim:DataSetX odim:usedDataInterface odim:DataInterfaceX .
}
