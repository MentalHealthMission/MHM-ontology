PREFIX connect: <http://connectdigitalstudy.com/ontology#>

ASK {
  # Measurement generated by sensing activity
  connect:HeartRateMeasurementX connect:wasGeneratedBy connect:SensingActivityX .

  # Computation uses app interface and produces derived feature
  connect:DerivedFeatureX connect:wasComputedBy connect:ComputationX .
  connect:ComputationX connect:usedAppInterface connect:AppInterfaceX .

  # Dataset attribution to participant and interface use
  connect:DataSetX connect:wasAttributedTo connect:ParticipantX .
  connect:DataSetX connect:usedDataInterface connect:DataInterfaceX .
}

