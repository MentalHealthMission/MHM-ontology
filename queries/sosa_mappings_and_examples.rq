PREFIX odim:    <http://connectdigitalstudy.com/ontology#>
PREFIX sosa:    <http://www.w3.org/ns/sosa/>
PREFIX rdfs:    <http://www.w3.org/2000/01/rdf-schema#>

ASK {
  # Class mapping exists
  odim:Measurement rdfs:subClassOf sosa:Observation .

  # Property mappings exist (via subPropertyOf on convenience props)
  odim:observedProperty rdfs:subPropertyOf sosa:observedProperty .
  odim:featureOfInterest rdfs:subPropertyOf sosa:hasFeatureOfInterest .

  # Examples have observedProperty and FOI
  odim:HeartRateMeasurementX odim:observedProperty odim:HeartRate ;
                             odim:featureOfInterest odim:ParticipantX .
  odim:HeartRateVariabilityMeasurementX odim:observedProperty odim:HeartRateVariability ;
                                        odim:featureOfInterest odim:ParticipantX .
  odim:ActivityMeasurementX odim:observedProperty odim:ActivityDuration ;
                            odim:featureOfInterest odim:ParticipantX .
  odim:SleepStageMeasurementX odim:observedProperty odim:SleepDuration ;
                              odim:featureOfInterest odim:ParticipantX .
}
